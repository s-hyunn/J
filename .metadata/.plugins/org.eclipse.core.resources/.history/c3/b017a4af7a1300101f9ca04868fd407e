package Day0407;

public class InfinityObjectArray {

	private Object[] objs = new Object[3];

	public InfinityObjectArray() {
		objs = new Object[3];
	}

	// objs배열에 데이터를 추가하는 기능의 메서드
	public void f1() {
		String s = "aaa";
		objs[0] = s;
		
	}
	public void f2() {
		String s = "bbb";
		objs[1] = s;
		
	}
	public void f3() {
		String s = "ccc";
		objs[2] = s;
		
	}
	
	public void showArray() {
		for(int i=0;i< objs.length;i++) {
			System.out.println(objs[i]);
		}
	}

}

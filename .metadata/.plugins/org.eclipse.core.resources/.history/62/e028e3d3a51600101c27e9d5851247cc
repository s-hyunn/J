	public static void main(String[] args) {
		//0회원정보(나중에 스캐너로 입력받을거임
		String id = "aaa";
		String pw = "1234";
		String name = "홍길동";
		String phone = "010-44";
		String grade = "A";
		
		//1회원정보 member인스턴스에 저장하기 //생성자로 바로 (안에 입력받아서 만들 수도 있음)
		Member m = new Member();
		m.setId(id);
		m.setPw(pw);
		m.setName(name);
		m.setPhone(phone);
		m.setGrade(grade);
		
		
		
		
		
		
		
		
	}


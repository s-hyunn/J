package Day0407;

public class Customertest {

	public static void main(String[] args) {
		Customer c1= new Customer(1,"홍길동","010-7470-2019",500000);
		Customer c2= new Customer(2,"춘향","010-7423-2219",1500000);
		
		System.out.println(c1);
		System.out.println(c2);
		
		c1.buy(500001);
		c1.buy(2222);
		
		
		
	}

}

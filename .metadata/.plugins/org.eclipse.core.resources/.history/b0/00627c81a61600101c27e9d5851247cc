package 같이연습;

import java.util.Scanner;

public class Testmain {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		//0회원정보(나중에 스캐너로 입력받을거임
		System.out.println();
		String id = "aaa";
		System.out.println();
		String pw = "1234";
		System.out.println();
		String name = "홍길동";
		System.out.println();
		String phone = "010-44";
		System.out.println();
		String grade = "A";
		
		//1회원정보 setter로 member인스턴스에 저장하기 //기본생성자, 생성자 만들어놓고 바로 (안에 입력받아서 만들 수도 있음)
		Member m = new Member();
		m.setId(id);
		m.setPw(pw);
		m.setName(name);
		m.setPhone(phone);
		m.setGrade(grade);
		
		System.out.println(m);
		
		
		
		
		
		
		
	}

}
